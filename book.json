  "plugins": ["disqus", "anchorjs", "search-languages", "-lunr", "yametrika", "expandable-chapters", "codetabs", "copy-code-button"],
  "pluginsConfig": {
    "disqus": {
      "shortName": "hftbattle"
    },
    "search_languages": {
      "lang": "ru"
    },
    "yametrika": {
      "number": 35257920,
      "settings": {
        "webvisor": true,
        "clickmap": true,
        "trackLinks": true,
        "accurateTrackBounce": true,
        "trackHash": true,
        "ut": "noindex"
      }
    }
  },
  "theme-default": {
    "styles": {
      "website": "build/gitbook.css"
    }
  }
}
