    "contest.local-pack.readme": "https://github.com/hftbattle/hftbattle/blob/master/README.md",
    "contest.local-pack.data.url": "https://www.dropbox.com/s/s1uxgs3vj3tgvr7/data.zip?dl=1",
    "contest.local-pack.virtual.url": "https://s3.eu-central-1.amazonaws.com/hftbattle/vm-hftbattle.ova",

    "blog.url": "http://blog.hftbattle.com",

    "data.training_days": "40",
    "data.control_days": "20",
    "data.final_days": "20",
    "data.small_day": "2015.09.29",
    "data.full_day": "2015.12.23",
    "git.url": "http://git-scm.com/download",
    "virtualbox.url": "https://www.virtualbox.org",
    "mingw.url": "http://sourceforge.net/projects/mingw-w64/",

    "final_testing_duration": 4,

    "brew.url": "http://brew.sh/",
    "cmake.url": "https://cmake.org/",
    "clion.url": "https://www.jetbrains.com/clion/download/",
    "python.url": "https://www.python.org/downloads/",

    "viewer.url": "http://viewer.hftbattle.com",
    "viewer.name": "viewer.hftbattle.com",

    "virtual.system": "Ubuntu 16.04",

    "trading.begin_time": "02:00:00",
    "trading.end_time": "23:45:00",
    "trading.daily_deals": "5-10",
    "trading.min_step": "25",
    "trading.lot_cost": "0.5",
    "trading.min_cost": "12.5",

    "constraint.symbol": "10^6",
    "constraint.string": "10^3",
    "constraint.symbol_in_string": "10^3",
    "constraint.logs_size": "100",
    "constraint.simulation_time": "10",

    "constraint.daily_control_submissions": "2",
    "constraint.position": "50",
    "constraint.stop_loss": "-50000$",
    "constraint.comission": "0.2",
    "constraint.roundtrip": "10"
