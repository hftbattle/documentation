    "contest.docs.url": "https://docs.hftbattle.com",
    "contest.docs.name": "docs.hftbattle.com",

    "contest.instrument": "ES",

    "contest.support.email": "help@hftbattle.com",
    "contest.support.slack.url": "https://slack.hftbattle.com",
