        "contest.arena.name": "arena.hftbattle.com",

        "contest.docs.url": "https://docs.hftbattle.com",
        "contest.docs.name": "docs.hftbattle.com",

        "contest.support.email": "help@hftbattle.com",
        "contest.support.slack.url": "https://slack.hftbattle.com",
        "contest.support.slack.name": "slack.hftbattle.com",

        "contest.local-pack.name": "github.com/hftbattle/hftbattle",
        "contest.local-pack.url": "https://github.com/hftbattle/hftbattle",
        "contest.local-pack.readme": "https://github.com/hftbattle/hftbattle/blob/master/README.md",
        "contest.local-pack.data.url": "https://www.dropbox.com/s/s1uxgs3vj3tgvr7/data.zip?dl=1",
