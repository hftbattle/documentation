{
  "gitbook": "3.2.2",
  "title": "HFT Battle",
  "variables": {
    "contest.name": "HFT Battle 2017",

    "gitbook.img": "http://192.168.7.159:4000/img",

    "contest.landing-page.url": "https://www.hftbattle.com",

    "contest.arena.url": "https://www.hftbattle.com/arena",
    "contest.arena.leaderboard.url": "https://www.hftbattle.com/arena/leaderboard",
    "contest.arena.name": "hftbattle.com/arena",

    "contest.docs.url": "https://docs.hftbattle.com",

    "contest.instrument": "ES",

    "contest.support.email": "help@hftbattle.com",

    "contest.local-pack.url": "https://github.com/hftbattle/hftbattle",
    "contest.local-pack.data.url": "https://www.dropbox.com/s/s1uxgs3vj3tgvr7/data.zip?dl=1",

    "blog.url": "http://blog.hftbattle.com",

    "data.training_days": "40",
    "data.control_days": "20",
    "data.final_days": "20",
    "data.small_day": "2015.09.29",
    "data.full_day": "2015.12.23",
    "git.url": "http://git-scm.com/download",
    "tdm-gcc.url": "http://tdm-gcc.tdragon.net",
    "tdm-gcc-download.url": "http://tdm-gcc.tdragon.net/download",

    "final_testing_duration": "4",

    "brew.url": "http://brew.sh/",
    "cmake.url": "https://cmake.org/",
    "clion-download.url": "https://www.jetbrains.com/clion/download/",
    "python.url": "https://www.python.org/downloads/",
    "std_deviation.ru_url": "https://ru.wikipedia.org/wiki/Среднеквадратическое_отклонение",
    "std_deviation.en_url": "https://en.wikipedia.org/wiki/Standard_deviation",

    "viewer.url": "http://viewer.hftbattle.com",
    "viewer.name": "viewer.hftbattle.com",

    "high.priority": "6",
    "medium.priority": "40",

    "trading.begin_time": "09:15:00",
    "trading.end_time": "15:30:00",
    "trading.daily_deals": "5-10",

    "constraint.symbol": "1&nbsp;000&nbsp;000",
    "constraint.string": "1000",
    "constraint.symbol_in_string": "1000",
    "constraint.logs_size": "100",
    "constraint.simulation_time": "10",

    "constraint.daily_control_submissions.ru.nom": "две",
    "constraint.daily_control_submissions.ru.gen": "двух",
    "constraint.daily_control_submissions.ru.ins": "двумя",
    "constraint.position": "50",
    "constraint.stop_loss": "-50000",
    "constraint.comission": "0.3",
    "constraint.round-trip": "10",

    "comission.equation": "0.3 * 5000 = 1500"
  },
  "plugins": ["language-picker", "edit-link", "disqus", "anchorjs", "search-languages", "-lunr", "yametrika", "expandable-chapters", "codetabs", "copy-code-button"],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/hftbattle/documentation/tree/asalikhov/new_version",
      "label": "Suggest an improvement"
    },
    "disqus": {
      "shortName": "hftbattle"
    },
    "search_languages": {
      "lang": "ru"
    },
    "yametrika": {
      "number": 35257920,
      "settings": {
        "webvisor": true,
        "clickmap": true,
        "trackLinks": true,
        "accurateTrackBounce": true,
        "trackHash": true,
        "ut": "noindex"
      }
    }
  }
}
