        "contest.instrument": "ES",

        "blog.url": "http://blog.hftbattle.com",

        "data.small_day": "2015.09.29",
        "data.full_day": "2015.12.23",
        "git.url": "http://git-scm.com/download",
        "virtualbox.url": "https://www.virtualbox.org",

        "brew.url": "http://brew.sh/",
        "cmake.url": "https://cmake.org/",
        "clion.url": "https://www.jetbrains.com/clion/download/",
        "python.url": "https://www.python.org/downloads/",

        "viewer.url": "http://viewer.hftbattle.com",
        "viewer.name": "viewer.hftbattle.com",

        "wiki.cme.url": "https://en.wikipedia.org/wiki/Чикагская_товарная_биржа",

        "constraint.symbol": "10^6",
        "constraint.string": "10^3",
        "constraint.symbol_in_string": "10^3",
        "constraint.logs_size": "100"
    },
    "plugins": ["disqus", "anchorjs", "search-languages", "-lunr", "yametrika", "expandable-chapters", "codetabs", "copy-code-button"],
    "pluginsConfig": {
        "disqus": {
            "shortName": "hftbattle"
        },
        "search_languages": {
            "lang": "ru"
        },
        "yametrika": {
            "number": 35257920,
            "settings": {
                "webvisor": true,
                "clickmap": true,
                "trackLinks": true,
                "accurateTrackBounce": true,
                "trackHash": true,
                "ut": "noindex"
            }
        }
    },
    "theme-default": {
      "styles": {
        "website": "build/gitbook.css"
      }
    }
}
